{
    "compilerOptions": {
        "target": "ES2018",
        "lib": ["ESNext"],
        "module": "ES2020",
        "moduleResolution": "Node",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noUnusedParameters": true,
        "resolveJsonModule": true,
        "outDir": "dist",
        "rootDir": "src"
    }
}
